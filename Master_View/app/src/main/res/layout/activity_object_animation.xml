<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.administrator.master_view.ui.ObjectAnimationActivity">

    
    <ImageView
    android:layout_width="100dp"
    android:layout_height="100dp"
    android:src="@drawable/p6"
    android:onClick="onIV6click"
    />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:onClick="onIV5click"
        android:src="@drawable/p5"/>

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/p4"
        android:onClick="onIV4click"
        />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/p3"
        android:onClick="onIV3click"
        />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="实现抛物线落体"
        android:onClick="onBallClick"
        />
</LinearLayout>
